# =============================================================================
# ENVIRONMENT VARIABLES
# =============================================================================

# Suppress Sass legacy JS API deprecation warnings
# These warnings are from sass-loader using the legacy API and don't affect functionality
# The warnings will be resolved when Create React App updates to the modern Sass API
SASS_SUPPRESS_WARNINGS=true

# Generate source maps in development for better debugging
GENERATE_SOURCEMAP=true

# Fast refresh for better development experience
FAST_REFRESH=true

# Disable webpack bundle analyzer popup in development
ANALYZE=false